{
  "name": "marsoltys/laravel-pckg-checker",
  "description": "Check installed composer packages.",
  "keywords": [
    "package",
    "composer",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mariusz Soltys",
      "email": "mar.soltys@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "laravel/framework": ">=5.1"
  },
  "autoload": {
    "psr-4": {
      "marsoltys\\laravel-pckg-checker\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "marsoltys\\laravel-pckg-checker\\PackageCheckerServiceProvider"
      ]
    }
  }
}
